{
  "version": 2,
  "builds": [
    {
      "src": "TITAN_OUTPUT_*/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/TITAN_OUTPUT_latest/web/dashboard/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/TITAN_OUTPUT_latest/web/dashboard/index.html"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/TITAN_OUTPUT_latest/web/blog/$1"
    },
    {
      "src": "/seo/(.*)",
      "dest": "/TITAN_OUTPUT_latest/web/seo/$1"
    },
    {
      "src": "/podcasts/(.*)",
      "dest": "/TITAN_OUTPUT_latest/web/podcasts/$1"
    },
    {
      "src": "/podcast.xml",
      "dest": "/TITAN_OUTPUT_latest/web/podcast.xml"
    },
    {
      "src": "/(.*)",
      "dest": "/TITAN_OUTPUT_latest/web/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
